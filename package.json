{
  "name": "@otedesco/cache",
  "version": "2.0.1",
  "type": "commonjs",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "pnpm": "8.6.10"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/otedesco/cache.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run clean && pnpm run type:dts && pnpm run build:main",
    "build:main": "swc ./src -d ./dist",
    "clean": "rimraf build coverage nyc_output",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.5",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "npmlog": "^7.0.1",
    "execa": "^5.1.1",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "@semantic-release/exec": "^6.0.3",
    "c8": "^8.0.1",
    "cspell": "^6.31.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "semantic-release-npm-github-publish": "^1.5.4",
    "source-map-support": "^0.5.21",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "winston": "^3.10.0"
  }
}
